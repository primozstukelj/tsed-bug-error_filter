    "@tsed/exceptions": "^7.41.1",
    "@tsed/json-mapper": "^7.41.1",
    "@tsed/logger": "^6.6.3",
    "@tsed/logger-file": "^6.6.3",
    "@tsed/openspec": "^7.41.1",
    "@tsed/platform-cache": "^7.41.1",
    "@tsed/platform-exceptions": "^7.41.1",
    "@tsed/platform-express": "^7.41.1",
